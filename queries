CREATE KEYSPACE cmpe274
WITH replication = {'class':'SimpleStrategy', 'replication_factor' : 3};


USE cmpe274;


CREATE TABLE share(
   share_id text PRIMARY KEY,
   Volume text,
   Symbol text,
   Adj_Close text,
   High text,
   Low text,
   Date text,
   Close text,
   Open text
   );

DROP TABLE share;

select COUNT(*) from share;


